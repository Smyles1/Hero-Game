pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
player = {}
player.x = 60
player.y = 60
player.sprite = 0
player.direct = 0-- 1=down 2=up 3=left 4=right
player.direct2 = 1 --2nd direction that does not change to 0 when player is not moving
player.flipx = false
player.flipy = false
player.spritewait = 0
player.celx = 0
player.cely = 0

racefinish = false

key = false

racetimer = 0
racestart = false

screenx = 0 --x and y of camera
screeny = 0

coldirect = nil--coldirect: direction of collision
rcol = 0 --right collision number
lcol = 0
ucol = 0
dcol = 0

arrowx = -20
arrowy = -20
arrowflipx = false
arrowflipy = false
arrowspr = 36
arrowdirect = 1 --direction player shoots arrow in (stays the same until shot again


function _init()
	music(10)
end

function _draw()
	cls()
	
	
	
	
	
	
	--key stuff in dungeon
	if key==false and screenx==768 and screeny==256 and player.y==32 and player.x>=24 and player.x<=40 then
	mset (100,36,31)
	key = true
	end
	
	
	map(0,0,0,0,10000,10000)
	
	
	
	shoot() --shooting function
	collision()--calls collisions
 arrowcollision()
 race() --race dungeon function
	trap() --room w/ green squares

	--key icon in corner
	if key == true then
	spr(43,screenx,screeny)
	end
	
	
	--locked door opening thing
	if screenx==768 and screeny==128 and player.x >= 108 and key==true then
	mset(111,22,31)
	mset(111,23,31)
	mset(111,24,31)
	mset(111,25,31)
	mset(110,22,31)
	mset(109,22,31)
	mset(108,22,31)
	key = false
	end
	
--[[print(player.x,20+screenx,20+screeny,8)
	print(player.y,40+screenx,20+screeny,8)
	print(screenx,20+screenx,30+screeny,8)
	print(screeny,40+screenx,30+screeny,8)
]]
	camera(screenx,screeny)--the screen in view (x and y in pixels)
	
	player.celx = (player.x + 4)/8 --gets celx
	player.cely = (player.y + 4)/8




	--detects if player is at entrance to dungeon
	if mget(screenx/8+player.celx+.5,screeny/8+player.cely) == 9 then music(19) player.x=800 screenx=640 screeny = 128 player.y=64 end
	
	--spritewait cycling
player.spritewait += 1
	if player.spritewait >= 2 then
		player.spritewait = 0
	end
	
	--player 
	spr(player.sprite,player.x+screenx,player.y+screeny,1,1,player.flipx,player.flipy)
end


function collision()

	rcol = fget(mget(player.celx+screenx/8+.5,player.cely+screeny/8))
	lcol = fget(mget(player.celx+screenx/8-.5,player.cely+screeny/8))
	
	dcol = fget(mget(player.celx+screenx/8,player.cely+screeny/8+.5))
	ucol = fget(mget(player.celx+screenx/8,player.cely+screeny/8-.5))
end


function shoot()
	spr(arrowspr,arrowx+screenx,arrowy+screeny,1,1,arrowflipx,arrowflipy)
	
	if arrowdirect==1 then --if direction of player when x is pressed is 1, shoot down
	arrowy += 2
	end
	
	if arrowdirect==2 then
	arrowy -= 2
	end
	
	if arrowdirect==3 then
	arrowx -= 2
	end
	
	if arrowdirect==4 then
	arrowx += 2
	end
	
	
	--makes it so you cant change arrow direction while flying
	if arrowx >= 128 or arrowx <= 0 or arrowy >= 128 or arrowy <= 0 then
	
	if player.direct2==1 then--changes direction of arrow based on player direction then
			arrowspr = 37
			arrowflipy = true
			arrowflipx = false
	end
	
	
	
	if player.direct2==2 then--changes direction of arrow based on player direction then
			arrowspr = 37
			arrowflipy = false
			arrowflipx = false
	end
	
	if arrowdirect==3 then
	arrowx -= 2
	end
	
	if player.direct2==3 then--changes direction of arrow based on player direction then
			arrowspr = 36
			arrowflipy = false
			arrowflipx = true
	end
	
	
	if player.direct2==4 then--changes direction of arrow based on player direction then
			arrowspr = 36
			arrowflipy = false
			arrowflipx = false
	end
	end
	
	if btn(5) then --shoot button
	if arrowx >= 128 or arrowx <= 0 or arrowy >= 128 or arrowy <= 0 then
	arrowx = player.x
	arrowy = player.y
	player.sprite = player.direct2 + 47
	arrowdirect = player.direct2
	end
	end
end


function trap()
if mget(player.celx+screenx/8,player.cely+screeny/8) == 62 then
player.x = 5
player.y = 58
sfx(51)
end
end

--function for the dungeo race
function race()
	if racestart == false and racefinish == false and screenx==768 and screeny==128 and player.y <= 42 then
 racetimer = 0
 racestart = true	
 mset(96,17,31)
	end
	
	if racestart == true and racefinish == false then
	racetimer += 1
	print (racetimer/30,58+screenx,90+screeny,8)
	print ("complete in < 9 sec",26+screenx,80+screeny,8)
	end
	
	if racetimer/30 >= 9 and screenx == 640 and screeny ==128 then
		racestart = false
		racetimer = 0
		racefinish = true
	end
	if racetimer/30 >= 9 and screenx == 768 and screeny == 128 then
		player.x = 60
		player.y = 60
		
		racestart = false
		racetimer = 0
	end
	
end


function arrowcollision()
	if mget((arrowx+4+screenx)/8,(arrowy+screeny)/8)==28 then
	music(-1)
	sfx(50)
	music(9)
	mset(46,8,15)
	end
end


function _update()

	--checks if a button is pressed
	if btn(0) or btn(1) or btn(2) or btn(3) then 
		--move up:
		if btn(2) and ucol == 0 then
			player.y -= 1
			player.direct2 = 2--see var call at top
			player.direct = 2 --direction facing
		end
		
		--move down:
		if btn(3) and dcol == 0 then
			player.y += 1
			player.direct = 1
			player.direct2 = 1
		end
		
		--move left:
		if btn(0) and lcol == 0then
			player.x -= 1
			player.direct = 3
			player.direct2 = 3
		end
		
		--move right:
		if btn(1) and rcol == 0 then
			player.x += 1
			player.direct = 4
			player.direct2 = 4
		end
	else
	
	--sets it to 0 if no directional button is pressed
		player.direct = 0 
	end
	
	
	--direction sprite changing
	if player.direct == 2 then--up
	
	--spritewait is 0 or 1 so sprite is 17 or 18
		player.sprite = 17 + player.spritewait
	end
	
	if player.direct == 1 then--down
		player.sprite = 1 + player.spritewait
	end
	
	if player.direct == 3 then--l
		player.sprite = 20 + player.spritewait
	end
	
	if player.direct == 4 then--r
		player.sprite = 4 + player.spritewait
	end
	if player.x >= 124 then --if player is at right edge of screen
		screenx += 128
		player.x = 4
	end
	if player.x <= 3 then --if player is at right edge of screen
		screenx -= 128
		player.x = 123
	end
	if player.y >= 124 then --if player is at right edge of screen
		screeny += 128
		player.y = 4
	end
	if player.y <= 3 then --if player is at right edge of screen
		screeny -= 128
		player.y = 123
	end
end


